{
  "name": "mocha-browserstack",
  "version": "0.0.1",
  "description": " Selenium examples for Mocha and BrowserStack Automate",
  "scripts": {
    "chrome": "npm install chromedriver@latest && mocha test/single.js --timeout=60000",
    "single": "browserstack-node-sdk mocha test/single.js --timeout=60000",
    "fail": "browserstack-node-sdk mocha test/fail.js --browserstack.config 'browserstack.fail.yml' --timeout=60000",
    "parallel": "browserstack-node-sdk mocha test/parallel/parallel*.js --browserstack.config 'browserstack.parallel.yml' --timeout=60000",
    "local": "browserstack-node-sdk mocha test/local.js --browserstack.config 'browserstack.local.yml' --timeout=60000",
    "mobile": "browserstack-node-sdk mocha test/mobile.js --browserstack.config 'browserstack.mobile.yml' --timeout=60000",
    "chrome-browserstack": "browserstack-node-sdk npm install chromedriver@latest && mocha test/single.js --timeout=60000",
    "postinstall": "npm update browserstack-node-sdk"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/browserstack/mocha-browserstack.git"
  },
  "keywords": [
    "mocha",
    "browserstack",
    "selenium",
    "automate",
    "examples"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/browserstack/mocha-browserstack/issues"
  },
  "homepage": "https://github.com/browserstack/mocha-browserstack#readme",
  "dependencies": {
    "browserstack-local": "^1.4.8",
    "chromedriver": "^112.0.0",
    "mocha": "^9.2.2",
    "selenium-webdriver": "4.1.2"
  },
  "devDependencies": {
    "browserstack-node-sdk": "^1.10.1"
  }
}